<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FormClub">
    <TextView
        android:id="@+id/textView4"
        android:layout_width="78dp"
        android:layout_height="30dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="108dp"
        android:text="ID:"
        android:textColor="@color/teal_200"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="78dp"
        android:layout_height="30dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="39dp"
        android:text="ClubName:"
        android:textColor="@color/teal_200"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="78dp"
        android:layout_height="30dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="39dp"
        android:text="ClubCreator:"
        android:textColor="@color/teal_200"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="78dp"
        android:layout_height="30dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="39dp"
        android:text="Howlarge:"
        android:textColor="@color/teal_200"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <EditText
        android:id="@+id/text_id1"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="103dp"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/text_clubname"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="30dp"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/text_id1" />

    <EditText
        android:id="@+id/text_Craeator"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="30dp"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/text_clubname" />

    <EditText
        android:id="@+id/text_large"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="30dp"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/text_Craeator" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="78dp"
        android:layout_height="30dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="39dp"
        android:text="Main:"
        android:textColor="@color/teal_200"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/text_MAIN"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="30dp"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/text_large" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="60dp"
        android:onClick="run"
        android:text="confirm"
        android:textColor="@color/teal_200"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />
</androidx.constraintlayout.widget.ConstraintLayout>